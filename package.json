{
  "name": "tamabot",
  "private": true,
  "engines": { "node": ">=18.18 <23" },
  "packageManager": "npm@10.8.2",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "preview": "next build && next start -p 3000",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "deploy:foundry": "forge script script/Deploy.s.sol:Deploy --rpc-url $RPC_URL --broadcast --verify --etherscan-api-key $ETHERSCAN_KEY",
    "filebase:pin": "tsx scripts/filebasePin.ts",
    "pin:sprite": "tsx scripts/pinSprite.ts",
    "oracle:sign": "tsx scripts/oracleSign.ts",
    "backfill": "tsx scripts/backfill.ts --from=1 --to=500 --delayMs=150"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "3.600.0",
    "@coinbase/onchainkit": "0.38.19",
    "@coinbase/wallet-sdk": "3.9.3",
    "@farcaster/miniapp-sdk": "latest",
    "@farcaster/miniapp-wagmi-connector": "1.1.0",
    "@filebase/sdk": "1.0.0",
    "@neynar/react": "1.2.20",
    "@pigment-css/react": "0.0.29",
    "@rainbow-me/rainbowkit": "2.2.1",
    "@supabase/supabase-js": "2.79.0",
    "@tanstack/react-query": "5.59.0",
    "@walletconnect/ethereum-provider": "2.21.1",
    "@wagmi/core": "2.22.1",
    "dotenv": "16.4.5",
    "next": "15.0.3",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "swr": "2.3.3",
    "viem": "2.38.5",
    "wagmi": "2.19.2",
    "pino-pretty": "10.3.1"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "5.0.0",
    "@types/node": "20.11.30",
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2",
    "hardhat": "2.26.5",
    "prettier": "3.3.3",
    "tsx": "4.7.0",
    "typescript": "5.5.4"
  },
  "overrides": {
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "@types/react": "19.0.2",
    "@types/react-dom": "19.0.2",
    "wagmi": "2.19.2",
    "@wagmi/core": "2.22.1",
    "viem": "2.38.5"
  }
}
